    "#Programa que solicita un numero al usuario y muestra la tabla de multiplicar de dicho numero\n",
    "\n",
    "Num = int (input(\"Por favor ingrese un numero: \"))\n",
    "print(\"La tabla de multiplicar del numero\", Num, \"es:\")\n",
    "\n",
    "for i in range (1,11):\n",
    "    print(Num, \"*\", i, \"=\", Num * i)"
